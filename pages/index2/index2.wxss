.nav-title {
  position: absolute;
  text-align: center;
  max-width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-size: 36rpx;
  color: #2c2b2b;
  font-weight: 600;
}

.account-bg1 {
  width: 40rpx;
  height: 40rpx;
  margin: 4rpx;
}

.btn1 {
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
  background-color: white;
  color: #999;
  border-radius: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.btnImg {
  width: 50rpx;
  height: 50rpx;
}

.btn1::after {
  border-radius: 98rpx;
  border: 0;
}

.btn2 {
  width: 200rpx;
  height: 200rpx;
  margin-top: 20rpx;
  background-color: white;
  color: #999;
  border-radius: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

.btn2::after {
  border: 0;
}

.viewt {
  position: fixed;
  text-align: center;
  bottom: 30rpx;
  left: 0;
  right: 0;
}

.user-agree {
  color: #7890fa;
  text-align: center;
  font-size: 80%;
}

.sv_order_list_1{
  z-index: 9999;
  top: auto;
  bottom: auto;
  width: 100%;
  overflow: hidden;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.ground1 {
  display: flex;
  position: relative;
  padding: 100rpx;
  background-color: #cf3529;
  color: #fff;
  margin-bottom: 10rpx;
}
